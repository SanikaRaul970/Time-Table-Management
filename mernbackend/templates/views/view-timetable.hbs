    <script>
        document.getElementById('division-select').addEventListener('change', function() {
            const be1Table = document.getElementById('BE1-timetable');
            const be2Table = document.getElementById('BE2-timetable');
            
            if (this.value === 'BE1') {
                be1Table.style.display = 'block';
                be2Table.style.display = 'none';
            } else {
                be1Table.style.display = 'none';
                be2Table.style.display = 'block';
            }
        });
    </script>
